                    android:orientation="vertical"
                    android:paddingStart="13dp"
                    android:paddingTop="17dp"
                    android:paddingEnd="13dp"
                    android:paddingBottom="17dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
