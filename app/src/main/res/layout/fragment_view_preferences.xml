                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_workingState"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
