<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#F5F5F5">



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/linearLayout_preferences">





            <!--Desired Partner Details-->
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                app:cardCornerRadius="2dp"
                app:cardElevation="2dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout_personalDetails"
                    android:orientation="vertical"
                    android:paddingStart="13dp"
                    android:paddingTop="17dp"
                    android:paddingEnd="13dp"
                    android:paddingBottom="17dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"

                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginStart="10dp"
                            android:padding="1dp"
                            android:src="@drawable/ic_action_person"
                            android:tint="@color/quantum_bluegrey800" />



                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginStart="12dp"
                            android:layout_weight="1"
                            android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:maxLines="1"
                            android:singleLine="true"
                            android:text="@string/desired_partner"
                            android:textColor="@color/quantum_bluegrey800"
                            android:textSize="17sp" />
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginEnd="0dp"
                            android:padding="2dp"
                            android:src="@drawable/ic_action_edit"
                            android:tint="@color/quantum_bluegrey800"
                            android:visibility="gone"/>


                    </LinearLayout>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/looking_for"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_lookingFor"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/height_should_be_between"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_height"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"


                        android:text="@string/age_should_be_between"

                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_age"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />



                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/colour"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_colour"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/marital_status"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_maritalStatus"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/family_status"
                        android:visibility="gone"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_familyStatus"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        android:visibility="gone"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/family_type"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_familyType"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/family_values"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_familyValues"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/diet"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_diet"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />




                </LinearLayout>

            </androidx.cardview.widget.CardView>



            <!--Religious Details-->
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                app:cardCornerRadius="2dp"
                app:cardElevation="2dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout_religiousDetails"
                    android:orientation="vertical"
                    android:paddingStart="13dp"
                    android:paddingTop="17dp"
                    android:paddingEnd="13dp"
                    android:paddingBottom="17dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"

                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginStart="10dp"
                            android:padding="1dp"
                            android:src="@drawable/ic_moon"
                            android:rotation="60"
                            android:tint="@color/quantum_bluegrey800" />



                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginStart="12dp"
                            android:layout_weight="1"
                            android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:maxLines="1"
                            android:singleLine="true"
                            android:text="@string/religious_details"
                            android:textColor="@color/quantum_bluegrey800"
                            android:textSize="17sp" />
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginEnd="0dp"
                            android:padding="2dp"
                            android:src="@drawable/ic_action_edit"
                            android:tint="@color/quantum_bluegrey800"
                            android:visibility="gone"/>


                    </LinearLayout>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/religion"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_religion"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/caste"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_caste"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />



                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/FieldName_TextView"
                        android:text="@string/sub_caste"
                        />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_subCaste"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />




                </LinearLayout>

            </androidx.cardview.widget.CardView>



            <!--Highest Qualification Details-->
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                app:cardCornerRadius="2dp"
                app:cardElevation="2dp"
