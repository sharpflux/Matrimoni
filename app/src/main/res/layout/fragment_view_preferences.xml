                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView_qualification"
                        style="@style/FieldValue_TextView"
                        android:text="@string/not_filled"
                        />




                </LinearLayout>

            </androidx.cardview.widget.CardView>



